    "test:integration:ci": "vitest test/integration --config vitest.config.integration.ts",
    "storybook:start": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1",
