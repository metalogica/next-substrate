{
  "name": "ideosphere-app",
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.4",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "app:build": "next build",
    "app:start": "next start",
    "app:dev": "next dev",
    "app:lint": "eslint --fix",
    "app:compile": "tsc --noEmit",
